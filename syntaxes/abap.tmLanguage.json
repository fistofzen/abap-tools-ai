{
    "name": "ABAP",
    "scopeName": "source.abap",
    "patterns": [
        {
            "include": "#comments"
        },
        {
            "include": "#strings"
        },
        {
            "include": "#keywords"
        },
        {
            "include": "#types"
        },
        {
            "include": "#numbers"
        },
        {
            "include": "#operators"
        }
    ],
    "repository": {
        "comments": {
            "patterns": [
                {
                    "match": "\\*.*$",
                    "name": "comment.line.asterisk.abap"
                },
                {
                    "begin": "\"",
                    "end": "$",
                    "name": "comment.line.quote.abap"
                }
            ]
        },
        "strings": {
            "patterns": [
                {
                    "begin": "'",
                    "end": "'",
                    "name": "string.quoted.single.abap"
                },
                {
                    "begin": "`",
                    "end": "`",
                    "name": "string.quoted.backtick.abap"
                },
                {
                    "begin": "\\|",
                    "end": "\\|",
                    "name": "string.quoted.other.abap"
                }
            ]
        },
        "keywords": {
            "patterns": [
                {
                    "match": "\\b(?i)(PROGRAM|REPORT|FUNCTION|METHOD|FORM|PERFORM|CALL|INCLUDE|MODULE|ENHANCEMENT|IMPLEMENTATION|DEFINITION|ENDMODULE|ENDFORM|ENDFUNCTION|ENDMETHOD|ENDENHANCEMENT|ENDCLASS|ENDINTERFACE|END-OF-SELECTION|END-OF-PAGE|AT|LOAD-OF-PROGRAM|START-OF-SELECTION|TOP-OF-PAGE|END-OF-FILE|GET|SET|MOVE|MOVE-CORRESPONDING|MULTIPLY|DIVIDE|ADD|SUBTRACT|COMPUTE|CONCATENATE|SEARCH|REPLACE|CONDENSE|CONVERT|SHIFT|EXTEND|FIND|COUNT|MATCH|OCCUR|OVERLAY|WRITE|FORMAT|NEW-LINE|SKIP|CLEAR|FREE|REFRESH|ASSIGN|UNASSIGN|READ|MODIFY|UPDATE|INSERT|DELETE|CLOSE|OPEN|FETCH|COLLECT|SORT|DESCRIBE|SUBMIT|AUTHORITY-CHECK|CHECK|MESSAGE|RAISE|CLEANUP|ASSERT|RETURN|THROW|EXIT|CONTINUE|LEAVE|RESUME)\\b",
